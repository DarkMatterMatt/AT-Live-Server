{
    "name": "at-live-server",
    "version": "2.0.0",
    "description": "An intermediary server to provide normalised live data to [commute.live](https://commute.live).",
    "main": "dist/index.js",
    "scripts": {
        "b": "npm run build",
        "bs": "npm run build && npm start",
        "build": "tsc",
        "lint": "eslint --fix src && tsc --noEmit",
        "start": "node --experimental-loader ./dist/loader.js dist/index.js",
        "test": "npm run build && mocha --paths \"dist/**/*.spec.ts\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DarkMatterMatt/AT-Live-Server.git"
    },
    "keywords": [
        "auckland",
        "public",
        "transport",
        "commute",
        "live",
        "tracker"
    ],
    "author": "Matt Moran",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/DarkMatterMatt/AT-Live-Server/issues"
    },
    "homepage": "https://github.com/DarkMatterMatt/AT-Live-Server#readme",
    "type": "module",
    "devDependencies": {
        "@types/node": "^16.11.7",
        "@types/triple-beam": "^1.3.2",
        "@types/ws": "^8.2.2",
        "@typescript-eslint/eslint-plugin": "^5.10.2",
        "@typescript-eslint/parser": "^5.10.2",
        "eslint": "^8.8.0",
        "gtfs-types": "^2.2.0",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "dotenv": "^16.0.0",
        "envalid": "^7.2.2",
        "fast-safe-stringify": "^2.1.1",
        "gtfs": "^3.2.4",
        "node-fetch": "^3.2.0",
        "node-graceful": "^3.1.0",
        "triple-beam": "^1.3.0",
        "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.6.0",
        "winston": "^3.5.1",
        "winston-daily-rotate-file": "^4.6.0",
        "ws": "^8.4.2"
    }
}
