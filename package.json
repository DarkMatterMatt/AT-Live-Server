{
    "name": "at-live-server",
    "version": "2.0.0",
    "description": "An intermediary server to provide normalised live data to [commute.live](https://commute.live).",
    "main": "dist/index.js",
    "scripts": {
        "b": "npm run build",
        "s": "npm run start",
        "bs": "npm run build && npm start",
        "build": "tsc",
        "d:b": "npm run docker:build",
        "d:s": "npm run docker:start",
        "d:bs": "npm run docker:build && npm run docker:start",
        "docker:build": "docker build -t commutelive .",
        "docker:start": "docker run --env-file .env --name commutelive -dp 9001:9001 commutelive",
        "lint": "eslint --fix src && tsc --noEmit",
        "start": "node --experimental-loader ./dist/loader.js dist/index.js",
        "test": "npm run build && mocha --paths \"dist/**/*.spec.ts\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DarkMatterMatt/AT-Live-Server.git"
    },
    "keywords": [
        "auckland",
        "public",
        "transport",
        "commute",
        "live",
        "tracker"
    ],
    "author": "Matt Moran",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/DarkMatterMatt/AT-Live-Server/issues"
    },
    "homepage": "https://github.com/DarkMatterMatt/AT-Live-Server#readme",
    "type": "module",
    "devDependencies": {
        "@types/node": "^18.11.7",
        "@types/ws": "^8.5.3",
        "@typescript-eslint/eslint-plugin": "^5.41.0",
        "@typescript-eslint/parser": "^5.41.0",
        "eslint": "^8.26.0",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "chalk": "^4.1.2",
        "dotenv": "^16.0.3",
        "envalid": "^7.3.1",
        "fast-safe-stringify": "^2.1.1",
        "gtfs": "^3.6.0",
        "node-fetch": "3.2.8",
        "node-graceful": "^3.1.0",
        "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.14.0",
        "winston": "^3.8.2",
        "winston-daily-rotate-file": "^4.7.1",
        "ws": "^8.10.0"
    }
}
